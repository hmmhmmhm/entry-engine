{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-react"
  ],
  "jsRules": {},
  "rules": {
      "no-console": false,
      "semicolon": [true, "never"],
      "no-unused-vars": ["warn"],
      "no-multi-spaces": false,
      "ter-indent": [ true, 4],
      "indent": [true, "spaces", 4],
      "jsx-wrap-multiline": false,
      "jsx-equals-spacing": [true, "always"]
  },
  "rulesDirectory": [
      "node_modules/tslint-eslint-rules/dist/rules"
  ]
}